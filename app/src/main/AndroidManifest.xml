<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.monitorwise">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_app_icom"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_app_icom_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MonitorWise">
        <activity
            android:name=".SplashActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:label="@string/app_name_2"
            android:theme="@style/Theme.MonitorWise.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Monitor.MonitorActivity"
            android:exported="true" />
        <activity android:name=".Dashboard.DashboardActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
    </application>
</manifest>